{
  "name": "pdf-annotate.js",
  "version": "0.0.0",
  "description": "Annotation layer for pdf.js",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "start": "cp ./node_modules/pdfjs-dist/build/pdf.js ./examples/ && cp ./node_modules/pdfjs-dist/build/pdf.worker.js ./examples/ && cp ./node_modules/pdfjs-dist/web/pdf_viewer.js ./examples/ && ./node_modules/.bin/webpack-dev-server --inline --config ./webpack.example.js --content-base examples/",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzabriskie/pdf-annotate.js.git"
  },
  "keywords": [
    "pdf",
    "annotation"
  ],
  "author": "Matt Zabriskie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzabriskie/pdf-annotate.js/issues"
  },
  "homepage": "https://github.com/mzabriskie/pdf-annotate.js#readme",
  "devDependencies": {
    "babel-core": "6.7.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.8",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "object-assign": "^4.0.1",
    "pdfjs-dist": "1.4.131",
    "twitter-text": "^1.13.4",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "create-stylesheet": "^0.2.0",
    "object-assign": "^4.0.1"
  }
}
