{
  "name": "pdf-annotate.js",
  "version": "0.0.0",
  "description": "Annotation layer for pdf.js",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "start": "cp ./node_modules/pdfjs-dist/build/pdf.worker.js ./examples/ && ./node_modules/.bin/webpack-dev-server --inline --config ./webpack.example.js --content-base examples/",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzabriskie/pdf-annotate.js.git"
  },
  "keywords": [
    "pdf",
    "annotation"
  ],
  "author": "Matt Zabriskie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzabriskie/pdf-annotate.js/issues"
  },
  "homepage": "https://github.com/mzabriskie/pdf-annotate.js#readme",
  "devDependencies": {
    "babel-core": "5.8.22",
    "babel-loader": "5.3.2",
    "coveralls": "^2.11.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.10",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "object-assign": "^4.0.1",
    "pdfjs-dist": "1.1.399",
    "twitter-text": "^1.13.3",
    "webpack": "1.11.0",
    "webpack-dev-server": "1.10.1"
  }
}
